{
  "name": "@gander-social-atproto/dev-env",
  "version": "0.4.6",
  "license": "MIT",
  "description": "Local development environment helper for atproto development",
  "keywords": [
    "atproto"
  ],
  "homepage": "https://github.com/gander-social/gander-social-atproto",
  "repository": {
    "type": "git",
    "url": "https://github.com/gander-social/gander-social-atproto",
    "directory": "packages/dev-env"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/bin.js",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "node -r dotenv/config dist/bin.js dotenv_config_path=.env.dev  ../dev-infra/with-external-db.sh psql $DB_POSTGRES_URL node --enable-source-maps dist/bin.js",
    "dev": "node -r dotenv/config dist/bin.js dotenv_config_path=.env.dev ../dev-infra/with-external-db.sh psql $DB_POSTGRES_URL node --enable-source-maps dist/bin.js --watch"
  },
  "engines": {
    "node": ">=18.7.0"
  },
  "dependencies": {
    "@did-plc/lib": "^0.0.1",
    "@did-plc/server": "^0.0.1",
    "@gander-social-atproto/api": "workspace:^",
    "@gander-social-atproto/bsync": "workspace:^",
    "@gander-social-atproto/common-web": "workspace:^",
    "@gander-social-atproto/crypto": "workspace:^",
    "@gander-social-atproto/gndr": "workspace:^",
    "@gander-social-atproto/identity": "workspace:^",
    "@gander-social-atproto/lexicon": "workspace:^",
    "@gander-social-atproto/ozone": "workspace:^",
    "@gander-social-atproto/pds": "workspace:^",
    "@gander-social-atproto/sync": "workspace:^",
    "@gander-social-atproto/syntax": "workspace:^",
    "@gander-social-atproto/xrpc-server": "workspace:^",
    "@types/pg": "^8.6.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "get-port": "^5.1.1",
    "multiformats": "^9.9.0",
    "pg": "^8.10.0",
    "uint8arrays": "3.0.0",
    "undici": "^6.14.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "typescript": "^5.6.3"
  }
}
