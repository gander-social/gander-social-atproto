{
  "name": "@gander-social-atproto/pds",
  "version": "0.5.8",
  "license": "MIT",
  "description": "Reference implementation of atproto Personal Data Server (PDS)",
  "keywords": [
    "gander-social-atproto",
    "pds"
  ],
  "homepage": "https://github.com/gander-social/gander-social-atproto",
  "repository": {
    "type": "git",
    "url": "https://github.com/gander-social/gander-social-atproto",
    "directory": "packages/pds"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.7.0"
  },
  "scripts": {
    "prepare": "[ \"$npm_config_production\" = true ] || puppeteer browsers install chrome",
    "codegen": "lex gen-server --yes ./src/lexicon ../../lexicons/com/gander-social-atproto/*/* ../../lexicons/app/gndr/*/* ../../lexicons/chat/gndr/*/* ../../lexicons/tools/ozone/*/*",
    "build": "tsc --build tsconfig.build.json",
    "postbuild": "node ./build.templates.js",
    "dev": "node ./build.templates.js --watch",
    "test": "../dev-infra/with-test-redis-and-db.sh jest",
    "test:sqlite": "jest",
    "test:sqlite-only": "jest --testPathIgnorePatterns /tests/proxied/*",
    "test:log": "tail -50 test.log | pino-pretty",
    "test:updateSnapshot": "../dev-infra/with-test-redis-and-db.sh jest --updateSnapshot",
    "migration:create": "ts-node ./bin/migration-create.ts"
  },
  "dependencies": {
    "@gander-atproto-nest/fetch-node": "workspace:*",
    "@gander-atproto-nest/xrpc-utils": "workspace:*",
    "@gander-social-atproto/api": "workspace:^",
    "@gander-social-atproto/aws": "workspace:^",
    "@gander-social-atproto/common": "workspace:^",
