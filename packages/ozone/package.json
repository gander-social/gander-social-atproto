    "kysely": "^0.22.0",
    "lande": "^1.0.10",
    "multiformats": "^9.9.0",
    "p-queue": "^6.6.2",
    "pg": "^8.10.0",
    "pino-http": "^8.2.1",
    "structured-headers": "^1.0.1",
    "typed-emitter": "^2.1.0",
    "uint8arrays": "3.0.0",
    "undici": "^6.14.1",
    "ws": "^8.12.0"
