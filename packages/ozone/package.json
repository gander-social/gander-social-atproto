  "devDependencies": {
    "@gander-social-atproto/lex-cli": "workspace:^",
    "@gander-social-atproto/pds": "workspace:^",
    "@did-plc/server": "^0.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.36",
    "@types/pg": "^8.6.6",
    "@types/qs": "^6.9.7",
