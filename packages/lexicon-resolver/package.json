{
  "name": "@gander-social-atproto/lexicon-resolver",
  "version": "0.2.0",
  "type": "commonjs",
  "license": "MIT",
  "description": "ATProto Lexicon resolution",
  "keywords": [
    "gander-social-atproto",
    "lexicon"
